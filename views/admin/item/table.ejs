<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <!-- <button class="btn btn-primary btn-sm" data-toggle="modal" data-target="#tambahItem"><i class="fas fa-plus"></i>
            tambah</button> -->
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>

                        <th>No</th>
                        <th>Title</th>
                        <th>Price</th>
                        <th>Country</th>
                        <th>City</th>
                        <th colspan="4" class="text-center">Action</th>

                    </tr>
                </thead>
                <tbody>
                    <% item.forEach((element,i) => { %>
                    <tr>
                        <td><%= i+1 %> </td>
                        <td><%= element.title %> </td>
                        <td><%= element.price %> </td>
                        <td><%= element.country %> </td>
                        <td><%= element.city %> </td>
                        <td>
                            <a href="/admin/item/show-image/<%= element.id %>" class="btn btn-info btn-sm btn-circle"><i
                                    class="fas fa-image"></i>
                            </a>
                        </td>
                        <td>
                            <a href="/admin/item/<%= element.id %> " class="btn btn-warning btn-sm btn-circle">
                                <i class="fas fa-edit"></i>
                            </a>
                        </td>
                        <td><a href="/admin/item/show-detail-item/<%= element.id %> "
                                class="btn btn-success btn-sm btn-circle"><i class="fas fa-plus"></i></a></td>
                        <td>
                            <form action="/admin/item/<%= element.id %>?_method=DELETE" method="post">
                                <button class="btn btn-danger btn-sm  btn-circle" type="submit"><i
                                        class="fas fa-trash"></i></button>
                            </form>
                        </td>
                    </tr>
                    <% }) %>

                </tbody>
            </table>
        </div>
    </div>
</div>